// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  // url      = env("DATABASE_URL")
  url      = "file:./test.db"
}

model User {
  id             String @unique @default(cuid())
  firstName      String
  lastName       String
  email          String
  hashedPassword String
}
